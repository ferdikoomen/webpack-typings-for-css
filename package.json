        "prettier": "prettier \"./src/**/*.js\" --check",
        "prettier:fix": "prettier \"./src/**/*.js\" --write",
        "codecov": "codecov --token=a1a1f1e5-572a-40fc-bf19-16eca1fe8016",
        "prepublish": "rollup --config"
