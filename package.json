        "rimraf": "3.0.2",
        "rollup": "2.63.0",
        "rollup-plugin-terser": "7.0.2",
        "sass-loader": "12.4.0",
        "style-loader": "3.3.1",
        "ts-loader": "9.2.6",
        "typescript": "4.5.4",
        "webpack": "5.65.0",
        "webpack-cli": "4.9.1",
        "webpack-dev-server": "4.7.2"
