        "node-sass": "4.14.1",
        "prettier": "2.1.2",
        "rimraf": "3.0.2",
        "rollup": "2.29.0",
        "rollup-plugin-terser": "7.0.2",
        "sass-loader": "10.0.2",
        "style-loader": "1.3.0",
        "ts-loader": "8.0.4",
        "typescript": "4.0.3",
        "webpack": "4.44.2",
