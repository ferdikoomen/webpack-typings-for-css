        "html-webpack-plugin": "5.3.1",
        "jest": "26.6.3",
        "jest-cli": "26.6.3",
        "less": "4.1.1",
        "less-loader": "8.0.0",
        "node-sass": "5.0.0",
        "prettier": "2.2.1",
        "rimraf": "3.0.2",
        "rollup": "2.43.1",
        "rollup-plugin-terser": "7.0.2",
        "sass-loader": "11.0.1",
        "style-loader": "2.0.0",
        "ts-loader": "8.0.18",
        "typescript": "4.2.3",
        "webpack": "5.28.0",
        "webpack-cli": "4.5.0",
        "webpack-dev-server": "3.11.2"
