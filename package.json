    },
    "license": "MIT",
    "maintainers": [
        {
            "name": "Ferdi Koomen",
            "email": "info@madebyferdi.com"
        }
    ],
    "keywords": [
        "webpack",
        "loader",
        "typescript",
        "typings",
        "generate",
        "scss",
        "less",
        "css"
    ],
    "main": "dist/index.js",
    "module": "dist/index.js",
