{
    "name": "webpack-typings-for-css",
    "version": "0.2.1",
    "description": "Webpack loader that generates TypeScript typings for CSS modules",
    "author": "Ferdi Koomen",
    "homepage": "https://github.com/ferdikoomen/webpack-typings-for-css",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ferdikoomen/webpack-typings-for-css.git"
    },
    "bugs": {
        "url": "https://github.com/ferdikoomen/webpack-typings-for-css/issues"
    },
    "license": "MIT",
    "maintainers": [
        {
            "name": "Ferdi Koomen",
            "email": "info@madebyferdi.com"
        }
    ],
    "keywords": [
        "webpack",
        "loader",
        "typescript",
        "typings",
        "generate",
        "scss",
        "less",
        "css"
    ],
    "main": "src/index.js",
    "module": "src/index.js",
    "files": [
        "src/index.js",
        "src/jest/identity-obj-proxy-esm.js",
        "typings/css.d.ts",
        "typings/less.d.ts",
        "typings/scss.d.ts"
    ],
    "scripts": {
        "clean": "rimraf ./test/dist ./test/*.d.ts",
        "build": "webpack",
        "run": "webpack-dev-server",
        "test": "webpack && jest",
        "eslint": "eslint \"./src/**/*.js\"",
        "eslint:fix": "eslint \"./src/**/*.js\" --fix",
        "prettier": "prettier \"./src/**/*.js\" --check",
        "prettier:fix": "prettier \"./src/**/*.js\" --write"
    },
    "dependencies": {
        "camelcase": "5.3.1",
        "path": "0.12.7"
    },
    "devDependencies": {
        "@types/node": "12.11.7",
        "css-loader": "3.2.0",
        "eslint": "6.6.0",
        "eslint-config-prettier": "6.5.0",
        "eslint-plugin-prettier": "3.1.1",
        "file-loader": "4.2.0",
        "html-webpack-plugin": "3.2.0",
        "jest": "24.9.0",
        "jest-cli": "24.9.0",
        "less": "3.10.3",
        "less-loader": "5.0.0",
        "node-sass": "4.13.0",
        "prettier": "1.18.2",
        "rimraf": "3.0.0",
        "sass-loader": "8.0.0",
        "style-loader": "1.0.0",
        "ts-loader": "6.2.1",
        "typescript": "3.6.4",
        "webpack": "4.41.2",
        "webpack-cli": "3.3.9",
        "webpack-dev-server": "3.9.0"
    }
}
